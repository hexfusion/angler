#! /usr/bin/env perl

use strict;
use warnings;

use FindBin;
 use lib "$FindBin::Bin/../../../nitesi/lib", 
	"$FindBin::Bin/../../../nitesi-dancer/lib";
use Dancer ':script';
use Dancer::Plugin::Nitesi;

query->delete('navigation', {type => 'menu'});

my $ret;

$ret = shop_navigation->create(name => 'Contact',
                        uri => 'contact',
                        description => '',
                        type => 'menu',
                        scope => 'top');

$ret = shop_navigation->create(name => 'About us',
                        uri => 'aboutus',
                        description => '',
                        type => 'menu',
                        scope => 'top');

$ret = shop_navigation->create(name => 'Shipping',
                        uri => 'shipping',
                        description => '',
                        type => 'menu',
                        scope => 'top');
